MAIN
	MOVE TEMP 0 
	BEGIN
		MOVE TEMP 1 
		BEGIN
			RETURN CALL Create_Fac()
		END
		HLOAD TEMP 2 TEMP 1 0
		HLOAD TEMP 3 TEMP 2 0

		RETURN CALL TEMP 3( TEMP 1 )
	END
	PRINT TEMP 0
END

Create_Fac [0]
BEGIN
	MOVE TEMP 0 HALLOCATE 4
	HSTORE TEMP 0 0 Fac_Compute

	MOVE TEMP 1 HALLOCATE 4
	HSTORE TEMP 1 0 TEMP 0

	RETURN TEMP 1
END

Fac_Compute [1]
BEGIN
	MOVE TEMP 2 
	BEGIN
		MOVE TEMP 3 TEMP 1
		HLOAD TEMP 4 TEMP 3 0
		RETURN TEMP 4
	END

	RETURN TEMP 2
END

