MAIN
	MOVE TEMP 0 
	BEGIN
		MOVE TEMP 1 
		BEGIN
			RETURN CALL Create_Test()
		END
		HLOAD TEMP 2 TEMP 1 0
		HLOAD TEMP 3 TEMP 2 0

		RETURN CALL TEMP 3( TEMP 1 )
	END
	PRINT TEMP 0
END

Create_Test [0]
BEGIN
	MOVE TEMP 0 HALLOCATE 12
	HSTORE TEMP 0 0 Test_start
	HSTORE TEMP 0 4 Test_mutual1
	HSTORE TEMP 0 8 Test_mutual2

	MOVE TEMP 1 HALLOCATE 12
	HSTORE TEMP 1 0 TEMP 0

	MOVE TEMP 2 HALLOCATE 4
	MOVE TEMP 2 0
	HSTORE TEMP 1 4 TEMP 2
	MOVE TEMP 3 HALLOCATE 4
	MOVE TEMP 3 0
	HSTORE TEMP 1 8 TEMP 3

	RETURN TEMP 1
END

Test_start [1]
BEGIN
	MOVE TEMP 1 4
	HSTORE TEMP 0 8 TEMP 1
	MOVE TEMP 2 0
	HSTORE TEMP 0 4 TEMP 2
	MOVE TEMP 3 
	BEGIN
		MOVE TEMP 4 TEMP 0
		HLOAD TEMP 5 TEMP 4 0
		HLOAD TEMP 6 TEMP 5 4

		RETURN CALL TEMP 6( TEMP 4 )
	END
	RETURN TEMP 3
END

Test_mutual1 [1]
BEGIN
	MOVE TEMP 2 MINUS 
	BEGIN
		HLOAD TEMP 3 TEMP 0 8
		RETURN TEMP 3
	END 1

	HSTORE TEMP 0 8 TEMP 2
	CJUMP LT 
	BEGIN
		HLOAD TEMP 4 TEMP 0 8
		RETURN TEMP 4
	END 0 L0
	MOVE TEMP 5 0
	HSTORE TEMP 0 4 TEMP 5
	JUMP L1
L0
	NOOP
	MOVE TEMP 6 
	BEGIN
		HLOAD TEMP 7 TEMP 0 4
		RETURN TEMP 7
	END
	PRINT TEMP 6
	MOVE TEMP 8 1
	HSTORE TEMP 0 4 TEMP 8
	MOVE TEMP 9 
	BEGIN
		MOVE TEMP 10 TEMP 0
		HLOAD TEMP 11 TEMP 10 0
		HLOAD TEMP 12 TEMP 11 8

		RETURN CALL TEMP 12( TEMP 10 )
	END
	MOVE TEMP 1 TEMP 9
L1
	NOOP
	MOVE TEMP 13 
	BEGIN
		HLOAD TEMP 14 TEMP 0 4
		RETURN TEMP 14
	END
	RETURN TEMP 13
END

Test_mutual2 [1]
BEGIN
	MOVE TEMP 2 MINUS 
	BEGIN
		HLOAD TEMP 3 TEMP 0 8
		RETURN TEMP 3
	END 1

	HSTORE TEMP 0 8 TEMP 2
	CJUMP LT 
	BEGIN
		HLOAD TEMP 4 TEMP 0 8
		RETURN TEMP 4
	END 0 L2
	MOVE TEMP 5 0
	HSTORE TEMP 0 4 TEMP 5
	JUMP L3
L2
	NOOP
	MOVE TEMP 6 
	BEGIN
		HLOAD TEMP 7 TEMP 0 4
		RETURN TEMP 7
	END
	PRINT TEMP 6
	MOVE TEMP 8 0
	HSTORE TEMP 0 4 TEMP 8
	MOVE TEMP 9 
	BEGIN
		MOVE TEMP 10 TEMP 0
		HLOAD TEMP 11 TEMP 10 0
		HLOAD TEMP 12 TEMP 11 4

		RETURN CALL TEMP 12( TEMP 10 )
	END
	MOVE TEMP 1 TEMP 9
L3
	NOOP
	MOVE TEMP 13 
	BEGIN
		HLOAD TEMP 14 TEMP 0 4
		RETURN TEMP 14
	END
	RETURN TEMP 13
END

