MAIN
	MOVE TEMP 0 
	BEGIN
		MOVE TEMP 1 
		BEGIN
			RETURN CALL Create_Ss()
		END
		HLOAD TEMP 2 TEMP 1 0
		HLOAD TEMP 3 TEMP 2 0
		MOVE TEMP 4 10
		RETURN CALL TEMP 3( TEMP 1 TEMP 4 )
	END
	PRINT TEMP 0
END

Create_Ss [0]
BEGIN
	MOVE TEMP 0 HALLOCATE 4
	HSTORE TEMP 0 0 Ss_Compute

	MOVE TEMP 1 HALLOCATE 8
	HSTORE TEMP 1 0 TEMP 0

	MOVE TEMP 2 HALLOCATE 4
	MOVE TEMP 2 0
	HSTORE TEMP 1 4 TEMP 2

	RETURN TEMP 1
END

Ss_Compute [2]
BEGIN
	MOVE TEMP 3 
	BEGIN
		RETURN CALL Create_Run()
	END
	HSTORE TEMP 0 4 TEMP 3
	MOVE TEMP 4 
	BEGIN
		MOVE TEMP 5 
		BEGIN
			HLOAD TEMP 6 TEMP 0 4
			RETURN TEMP 6
		END
		HLOAD TEMP 7 TEMP 5 0
		HLOAD TEMP 8 TEMP 7 0
		MOVE TEMP 9 TEMP 1
		RETURN CALL TEMP 8( TEMP 5 TEMP 9 )
	END
	MOVE TEMP 2 TEMP 4
	MOVE TEMP 10 TEMP 1
	RETURN TEMP 10
END

Create_Run [0]
BEGIN
	MOVE TEMP 0 HALLOCATE 4
	HSTORE TEMP 0 0 Run_foo

	MOVE TEMP 1 HALLOCATE 4
	HSTORE TEMP 1 0 TEMP 0

	RETURN TEMP 1
END

Run_foo [2]
BEGIN
	MOVE TEMP 3 
	BEGIN
		RETURN CALL Create_Run()
	END
	MOVE TEMP 2 TEMP 3
	MOVE TEMP 4 TEMP 2
	RETURN TEMP 4
END

