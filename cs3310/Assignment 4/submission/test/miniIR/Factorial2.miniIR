MAIN
	MOVE TEMP 0 
	BEGIN
		MOVE TEMP 1 
		BEGIN
			RETURN CALL Create_Fac()
		END
		HLOAD TEMP 2 TEMP 1 0
		HLOAD TEMP 3 TEMP 2 0
		MOVE TEMP 4 10
		RETURN CALL TEMP 3( TEMP 1 TEMP 4 )
	END
	PRINT TEMP 0
END

Create_Fac [0]
BEGIN
	MOVE TEMP 0 HALLOCATE 4
	HSTORE TEMP 0 0 Fac_ComputeFac

	MOVE TEMP 1 HALLOCATE 4
	HSTORE TEMP 1 0 TEMP 0

	RETURN TEMP 1
END

Fac_ComputeFac [2]
BEGIN
	MOVE TEMP 4 
	BEGIN
		RETURN CALL Create_Hello()
	END
	MOVE TEMP 2 TEMP 4
	CJUMP LT TEMP 1 1 L0
	MOVE TEMP 5 1
	MOVE TEMP 3 TEMP 5
	JUMP L1
L0
	NOOP
	MOVE TEMP 6 TIMES TEMP 1 
	BEGIN
		MOVE TEMP 7 
		BEGIN
			MOVE TEMP 8 TEMP 0
			HLOAD TEMP 9 TEMP 8 0
			HLOAD TEMP 10 TEMP 9 0
			MOVE TEMP 11 MINUS TEMP 1 1

			RETURN CALL TEMP 10( TEMP 8 TEMP 11 )
		END
		RETURN TEMP 7
	END
	MOVE TEMP 3 TEMP 6
L1
	NOOP
	MOVE TEMP 12 TEMP 3
	RETURN TEMP 12
END

Create_Hello [0]
BEGIN
	MOVE TEMP 0 HALLOCATE 4
	HSTORE TEMP 0 0 Hello_hi

	MOVE TEMP 1 HALLOCATE 4
	HSTORE TEMP 1 0 TEMP 0

	RETURN TEMP 1
END

Hello_hi [1]
BEGIN
	MOVE TEMP 1 0
	RETURN TEMP 1
END

