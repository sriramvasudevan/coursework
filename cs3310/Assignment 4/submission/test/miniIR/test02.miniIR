MAIN
	MOVE TEMP 0 
	BEGIN
		MOVE TEMP 1 
		BEGIN
			RETURN CALL Create_Test()
		END
		HLOAD TEMP 2 TEMP 1 0
		HLOAD TEMP 3 TEMP 2 0

		RETURN CALL TEMP 3( TEMP 1 )
	END
	PRINT TEMP 0
END

Create_Test [0]
BEGIN
	MOVE TEMP 0 HALLOCATE 4
	HSTORE TEMP 0 0 Test_start

	MOVE TEMP 1 HALLOCATE 4
	HSTORE TEMP 1 0 TEMP 0

	RETURN TEMP 1
END

Test_start [1]
BEGIN
	MOVE TEMP 1 PLUS 10 30
	RETURN TEMP 1
END

