MAIN
	MOVE TEMP 0 
	BEGIN
		MOVE TEMP 1 
		BEGIN
			RETURN CALL Create_Test()
		END
		HLOAD TEMP 2 TEMP 1 0
		HLOAD TEMP 3 TEMP 2 0

		RETURN CALL TEMP 3( TEMP 1 )
	END
	PRINT TEMP 0
END

Create_Test [0]
BEGIN
	MOVE TEMP 0 HALLOCATE 4
	HSTORE TEMP 0 0 Test_start

	MOVE TEMP 1 HALLOCATE 8
	HSTORE TEMP 1 0 TEMP 0

	MOVE TEMP 2 HALLOCATE 4
	MOVE TEMP 2 0
	HSTORE TEMP 1 4 TEMP 2

	RETURN TEMP 1
END

Test_start [1]
BEGIN
	MOVE TEMP 2 10
	HSTORE TEMP 0 4 TEMP 2
	MOVE TEMP 3 LT 
	BEGIN
		HLOAD TEMP 4 TEMP 0 4
		RETURN TEMP 4
	END 20
	MOVE TEMP 1 TEMP 3
	CJUMP TEMP 1 L0
	MOVE TEMP 5 1
	PRINT TEMP 5
	JUMP L1
L0
	NOOP
	MOVE TEMP 6 0
	PRINT TEMP 6
L1
	NOOP
	MOVE TEMP 7 
	BEGIN
		HLOAD TEMP 8 TEMP 0 4
		RETURN TEMP 8
	END
	RETURN TEMP 7
END

