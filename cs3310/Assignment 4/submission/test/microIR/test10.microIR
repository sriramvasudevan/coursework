MAIN
	MOVE TEMP 500 CALL Create_Test ( )
	MOVE TEMP 1 TEMP 500
	MOVE TEMP 501 TEMP 1
	HLOAD TEMP 2 TEMP 501 0
	MOVE TEMP 502 TEMP 2
	HLOAD TEMP 3 TEMP 502 0
	MOVE TEMP 503 TEMP 1
	MOVE TEMP 504 CALL TEMP 3 ( TEMP 503 )
	MOVE TEMP 0 TEMP 504
	PRINT TEMP 0
END

Create_Test [0]
BEGIN
	MOVE TEMP 505 HALLOCATE 8
	MOVE TEMP 0 TEMP 505
	MOVE TEMP 506 TEMP 0
	MOVE TEMP 507 Test_start
	HSTORE TEMP 506 0 TEMP 507
	MOVE TEMP 508 TEMP 0
	MOVE TEMP 509 Test_next
	HSTORE TEMP 508 4 TEMP 509
	MOVE TEMP 510 HALLOCATE 8
	MOVE TEMP 1 TEMP 510
	MOVE TEMP 511 TEMP 1
	MOVE TEMP 512 TEMP 0
	HSTORE TEMP 511 0 TEMP 512
	MOVE TEMP 513 HALLOCATE 4
	MOVE TEMP 2 TEMP 513
	MOVE TEMP 2 0
	MOVE TEMP 514 TEMP 1
	MOVE TEMP 515 TEMP 2
	HSTORE TEMP 514 4 TEMP 515
	RETURN TEMP 1
END 

Test_start [1]
BEGIN
	MOVE TEMP 2 10
	MOVE TEMP 516 TEMP 0
	MOVE TEMP 517 TEMP 2
	HSTORE TEMP 516 4 TEMP 517
	MOVE TEMP 518 CALL Create_Test ( )
	MOVE TEMP 3 TEMP 518
	MOVE TEMP 1 TEMP 3
	MOVE TEMP 5 TEMP 1
	MOVE TEMP 519 TEMP 5
	HLOAD TEMP 6 TEMP 519 0
	MOVE TEMP 520 TEMP 6
	HLOAD TEMP 7 TEMP 520 4
	MOVE TEMP 521 TEMP 5
	MOVE TEMP 522 CALL TEMP 7 ( TEMP 521 )
	MOVE TEMP 4 TEMP 522
	MOVE TEMP 523 TEMP 0
	MOVE TEMP 524 TEMP 4
	HSTORE TEMP 523 4 TEMP 524
	MOVE TEMP 525 TEMP 0
	HLOAD TEMP 9 TEMP 525 4
	MOVE TEMP 8 TEMP 9
	RETURN TEMP 8
END 

Test_next [1]
BEGIN
	MOVE TEMP 1 20
	RETURN TEMP 1
END 
