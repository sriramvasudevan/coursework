MAIN
	MOVE TEMP 0 
	BEGIN
		MOVE TEMP 1 
		BEGIN
			RETURN CALL Create_somename()
		END
		HLOAD TEMP 2 TEMP 1 0
		HLOAD TEMP 3 TEMP 2 0
		MOVE TEMP 4 5
		RETURN CALL TEMP 3( TEMP 1 TEMP 4 )
	END
	PRINT TEMP 0
END

Create_somename [0]
BEGIN
	MOVE TEMP 0 HALLOCATE 4
	HSTORE TEMP 0 0 somename_foo

	MOVE TEMP 1 HALLOCATE 12
	HSTORE TEMP 1 0 TEMP 0

	MOVE TEMP 2 HALLOCATE 4
	MOVE TEMP 2 0
	HSTORE TEMP 1 4 TEMP 2
	MOVE TEMP 3 HALLOCATE 4
	MOVE TEMP 3 0
	HSTORE TEMP 1 8 TEMP 3

	RETURN TEMP 1
END

somename_foo [2]
BEGIN
	MOVE TEMP 2 
	BEGIN
		MOVE TEMP 3 TEMP 1
		MOVE TEMP 4 HALLOCATE TIMES 4 PLUS 1 TEMP 3
		HSTORE TEMP 4 0 TEMP 3
		MOVE TEMP 5 0
L0
		CJUMP LT TEMP 5 TEMP 3 L1
		MOVE TEMP 6 PLUS 1 TEMP 5
		MOVE TEMP 7 TIMES 4 TEMP 6
		MOVE TEMP 8 PLUS TEMP 4 TEMP 7
		HSTORE TEMP 8 0 0
		MOVE TEMP 5 PLUS TEMP 5 1
		JUMP L0
L1
		NOOP
		RETURN TEMP 4
	END
	HSTORE TEMP 0 8 TEMP 2
	MOVE TEMP 9 PLUS 1 
	BEGIN
		HLOAD TEMP 10 TEMP 0 4
		RETURN TEMP 10
	END
	MOVE TEMP 11 
	BEGIN
		MOVE TEMP 12 
		BEGIN
			HLOAD TEMP 13 TEMP 0 8
			RETURN TEMP 13
		END
		MOVE TEMP 14 PLUS 1 
		BEGIN
			HLOAD TEMP 15 TEMP 0 4
			RETURN TEMP 15
		END
		MOVE TEMP 16 TIMES 4 TEMP 14
		MOVE TEMP 17 PLUS TEMP 12 TEMP 16
		HLOAD TEMP 18 TEMP 17 0
		RETURN TEMP 18
	END

	MOVE TEMP 19 TIMES 4 TEMP 9
	HLOAD TEMP 20 TEMP 0 8
	MOVE TEMP 21 PLUS TEMP 20 TEMP 19
	HSTORE TEMP 21 0 TEMP 11
	HSTORE TEMP 0 8 TEMP 20
	MOVE TEMP 22 0
	RETURN TEMP 22
END

