MAIN
	MOVE TEMP 0 
	BEGIN
		MOVE TEMP 1 
		BEGIN
			RETURN CALL Create_simpleAdd()
		END
		HLOAD TEMP 2 TEMP 1 0
		HLOAD TEMP 3 TEMP 2 0
		MOVE TEMP 4 2
		RETURN CALL TEMP 3( TEMP 1 TEMP 4 )
	END
	PRINT TEMP 0
END

Create_simpleAdd [0]
BEGIN
	MOVE TEMP 0 HALLOCATE 4
	HSTORE TEMP 0 0 simpleAdd_add

	MOVE TEMP 1 HALLOCATE 4
	HSTORE TEMP 1 0 TEMP 0

	RETURN TEMP 1
END

simpleAdd_add [2]
BEGIN
	MOVE TEMP 3 PLUS TEMP 1 TEMP 1
	MOVE TEMP 2 TEMP 3
	MOVE TEMP 4 TEMP 2
	RETURN TEMP 4
END

