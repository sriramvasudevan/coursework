MAIN
	MOVE TEMP 0 
	BEGIN
		MOVE TEMP 1 
		BEGIN
			RETURN CALL Create_somename()
		END
		HLOAD TEMP 2 TEMP 1 0
		HLOAD TEMP 3 TEMP 2 0
		MOVE TEMP 4 8
		MOVE TEMP 5 6
		RETURN CALL TEMP 3( TEMP 1 TEMP 4 TEMP 5 )
	END
	PRINT TEMP 0
END

Create_somename [0]
BEGIN
	MOVE TEMP 0 HALLOCATE 4
	HSTORE TEMP 0 0 somename_foo

	MOVE TEMP 1 HALLOCATE 4
	HSTORE TEMP 1 0 TEMP 0

	RETURN TEMP 1
END

somename_foo [3]
BEGIN
	MOVE TEMP 3 TIMES TEMP 1 TEMP 2
	MOVE TEMP 2 TEMP 3
	MOVE TEMP 4 0
	RETURN TEMP 4
END

