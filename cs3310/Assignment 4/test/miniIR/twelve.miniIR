MAIN
	MOVE TEMP 0 
	BEGIN
		MOVE TEMP 1 
		BEGIN
			RETURN CALL Create_simpleLookup()
		END
		HLOAD TEMP 2 TEMP 1 0
		HLOAD TEMP 3 TEMP 2 0
		MOVE TEMP 4 4
		RETURN CALL TEMP 3( TEMP 1 TEMP 4 )
	END
	PRINT TEMP 0
END

Create_simpleLookup [0]
BEGIN
	MOVE TEMP 0 HALLOCATE 4
	HSTORE TEMP 0 0 simpleLookup_look

	MOVE TEMP 1 HALLOCATE 4
	HSTORE TEMP 1 0 TEMP 0

	RETURN TEMP 1
END

simpleLookup_look [2]
BEGIN
	MOVE TEMP 4 
	BEGIN
		MOVE TEMP 5 TEMP 1
		MOVE TEMP 6 HALLOCATE TIMES 4 PLUS 1 TEMP 5
		HSTORE TEMP 6 0 TEMP 5
		MOVE TEMP 7 0
L0
		CJUMP LT TEMP 7 TEMP 5 L1
		MOVE TEMP 8 PLUS 1 TEMP 7
		MOVE TEMP 9 TIMES 4 TEMP 8
		MOVE TEMP 10 PLUS TEMP 6 TEMP 9
		HSTORE TEMP 10 0 0
		MOVE TEMP 7 PLUS TEMP 7 1
		JUMP L0
L1
		NOOP
		RETURN TEMP 6
	END
	MOVE TEMP 2 TEMP 4
	MOVE TEMP 11 PLUS 1 2
	MOVE TEMP 12 TEMP 1
	MOVE TEMP 13 TIMES 4 TEMP 11
	MOVE TEMP 14 PLUS TEMP 2 TEMP 13
	HSTORE TEMP 14 0 TEMP 12
	MOVE TEMP 15 
	BEGIN
		MOVE TEMP 16 TEMP 2
		HLOAD TEMP 17 TEMP 16 0
		RETURN TEMP 17
	END

	MOVE TEMP 3 TEMP 15
	MOVE TEMP 18 TEMP 3
	RETURN TEMP 18
END

