MAIN
	MOVE TEMP 0 
	BEGIN
		MOVE TEMP 1 
		BEGIN
			RETURN CALL Create_Test()
		END
		HLOAD TEMP 2 TEMP 1 0
		HLOAD TEMP 3 TEMP 2 4

		RETURN CALL TEMP 3( TEMP 1 )
	END
	PRINT TEMP 0
END

Create_Test [0]
BEGIN
	MOVE TEMP 0 HALLOCATE 12
	HSTORE TEMP 0 0 Test_second
	HSTORE TEMP 0 4 Test_start
	HSTORE TEMP 0 8 Test_first

	MOVE TEMP 1 HALLOCATE 8
	HSTORE TEMP 1 0 TEMP 0

	MOVE TEMP 2 HALLOCATE 4
	MOVE TEMP 2 0
	HSTORE TEMP 1 4 TEMP 2

	RETURN TEMP 1
END

Test_start [1]
BEGIN
	MOVE TEMP 2 
	BEGIN
		RETURN CALL Create_Test()
	END
	MOVE TEMP 1 TEMP 2
	MOVE TEMP 3 10
	HSTORE TEMP 0 4 TEMP 3
	MOVE TEMP 4 
	BEGIN
		MOVE TEMP 5 TEMP 1
		HLOAD TEMP 6 TEMP 5 0
		HLOAD TEMP 7 TEMP 6 8
		MOVE TEMP 8 
	BEGIN
		MOVE TEMP 9 
		BEGIN
			RETURN CALL Create_Test()
		END
		HLOAD TEMP 10 TEMP 9 0
		HLOAD TEMP 11 TEMP 10 0

		RETURN CALL TEMP 11( TEMP 9 )
	END
		RETURN CALL TEMP 7( TEMP 5 TEMP 8 )
	END
	HSTORE TEMP 0 4 TEMP 4
	MOVE TEMP 12 
	BEGIN
		HLOAD TEMP 13 TEMP 0 4
		RETURN TEMP 13
	END
	RETURN TEMP 12
END

Test_first [2]
BEGIN
	MOVE TEMP 2 TEMP 1
	RETURN TEMP 2
END

Test_second [1]
BEGIN
	MOVE TEMP 1 30
	RETURN TEMP 1
END

