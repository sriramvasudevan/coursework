MAIN
	MOVE TEMP 0 
	BEGIN
		MOVE TEMP 1 
		BEGIN
			RETURN CALL Create_Test()
		END
		HLOAD TEMP 2 TEMP 1 0
		HLOAD TEMP 3 TEMP 2 0

		RETURN CALL TEMP 3( TEMP 1 )
	END
	PRINT TEMP 0
END

Create_Test [0]
BEGIN
	MOVE TEMP 0 HALLOCATE 4
	HSTORE TEMP 0 0 Test_start

	MOVE TEMP 1 HALLOCATE 4
	HSTORE TEMP 1 0 TEMP 0

	RETURN TEMP 1
END

Test_start [1]
BEGIN
	CJUMP 	BEGIN
		MOVE TEMP 0 0
		CJUMP 1 L0
		CJUMP 0 L0
MOVE TEMP 0 1

L0
		NOOP
		RETURN TEMP 0
	END
 L1
	MOVE TEMP 2 0
	MOVE TEMP 1 TEMP 2
	JUMP L2
L1
	NOOP
	MOVE TEMP 3 1
	MOVE TEMP 1 TEMP 3
L2
	NOOP
	MOVE TEMP 4 TEMP 1
	RETURN TEMP 4
END

