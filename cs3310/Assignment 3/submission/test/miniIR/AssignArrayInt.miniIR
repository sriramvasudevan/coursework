MAIN
	MOVE TEMP 0 
	BEGIN
		MOVE TEMP 1 
		BEGIN
			RETURN CALL Create_Ss()
		END
		HLOAD TEMP 2 TEMP 1 0
		HLOAD TEMP 3 TEMP 2 0
		MOVE TEMP 4 1
		RETURN CALL TEMP 3( TEMP 1 TEMP 4 )
	END
	PRINT TEMP 0
END

Create_Ss [0]
BEGIN
	MOVE TEMP 0 HALLOCATE 4
	HSTORE TEMP 0 0 Ss_Compute

	MOVE TEMP 1 HALLOCATE 8
	HSTORE TEMP 1 0 TEMP 0

	MOVE TEMP 2 HALLOCATE 4
	MOVE TEMP 2 0
	HSTORE TEMP 1 4 TEMP 2

	RETURN TEMP 1
END

Ss_Compute [2]
BEGIN
	MOVE TEMP 3 0
	MOVE TEMP 2 TEMP 3
	MOVE TEMP 4 
	BEGIN
		MOVE TEMP 5 9
		MOVE TEMP 6 HALLOCATE TIMES 4 PLUS 1 TEMP 5
		HSTORE TEMP 6 0 TEMP 5
		MOVE TEMP 7 0
L0
		CJUMP LT TEMP 7 TEMP 5 L1
		MOVE TEMP 8 PLUS 1 TEMP 7
		MOVE TEMP 9 TIMES 4 TEMP 8
		MOVE TEMP 10 PLUS TEMP 6 TEMP 9
		HSTORE TEMP 10 0 0
		MOVE TEMP 7 PLUS TEMP 7 1
		JUMP L0
L1
		NOOP
		RETURN TEMP 6
	END
	HSTORE TEMP 0 4 TEMP 4
	MOVE TEMP 11 PLUS 1 TEMP 2
	MOVE TEMP 12 
	BEGIN
		MOVE TEMP 13 PLUS TEMP 2 
		BEGIN
			MOVE TEMP 14 TEMP 2
			RETURN TEMP 14
		END
		RETURN TEMP 13
	END
	MOVE TEMP 15 TIMES 4 TEMP 11
	HLOAD TEMP 16 TEMP 0 4
	MOVE TEMP 17 PLUS TEMP 16 TEMP 15
	HSTORE TEMP 17 0 TEMP 12
	HSTORE TEMP 0 4 TEMP 16
	MOVE TEMP 18 10
	RETURN TEMP 18
END

