MAIN
	MOVE TEMP 0 
	BEGIN
		MOVE TEMP 1 
		BEGIN
			RETURN CALL Create_Test()
		END
		HLOAD TEMP 2 TEMP 1 0
		HLOAD TEMP 3 TEMP 2 0

		RETURN CALL TEMP 3( TEMP 1 )
	END
	PRINT TEMP 0
END

Create_Test [0]
BEGIN
	MOVE TEMP 0 HALLOCATE 4
	HSTORE TEMP 0 0 Test_start

	MOVE TEMP 1 HALLOCATE 8
	HSTORE TEMP 1 0 TEMP 0

	MOVE TEMP 2 HALLOCATE 4
	MOVE TEMP 2 0
	HSTORE TEMP 1 4 TEMP 2

	RETURN TEMP 1
END

Test_start [1]
BEGIN
	MOVE TEMP 1 60
	HSTORE TEMP 0 4 TEMP 1
	MOVE TEMP 2 
	BEGIN
		HLOAD TEMP 3 TEMP 0 4
		RETURN TEMP 3
	END
	PRINT TEMP 2
	MOVE TEMP 4 PLUS 
	BEGIN
		HLOAD TEMP 5 TEMP 0 4
		RETURN TEMP 5
	END 10
	HSTORE TEMP 0 4 TEMP 4
	MOVE TEMP 6 
	BEGIN
		HLOAD TEMP 7 TEMP 0 4
		RETURN TEMP 7
	END
	RETURN TEMP 6
END

