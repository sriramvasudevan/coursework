MAIN
	MOVE TEMP 0 
	BEGIN
		MOVE TEMP 1 
		BEGIN
			RETURN CALL Create_Test()
		END
		HLOAD TEMP 2 TEMP 1 0
		HLOAD TEMP 3 TEMP 2 0

		RETURN CALL TEMP 3( TEMP 1 )
	END
	PRINT TEMP 0
END

Create_Test [0]
BEGIN
	MOVE TEMP 0 HALLOCATE 4
	HSTORE TEMP 0 0 Test_start

	MOVE TEMP 1 HALLOCATE 8
	HSTORE TEMP 1 0 TEMP 0

	MOVE TEMP 2 HALLOCATE 4
	MOVE TEMP 2 0
	HSTORE TEMP 1 4 TEMP 2

	RETURN TEMP 1
END

Test_start [1]
BEGIN
	MOVE TEMP 2 11
	HSTORE TEMP 0 4 TEMP 2
	MOVE TEMP 3 1
	MOVE TEMP 1 TEMP 3
L0
	CJUMP LT 
	BEGIN
		HLOAD TEMP 4 TEMP 0 4
		RETURN TEMP 4
	END 15 L1
	CJUMP TEMP 1 L2
	MOVE TEMP 5 1
	PRINT TEMP 5
	JUMP L3
L2
	MOVE TEMP 6 0
	PRINT TEMP 6
L3
	NOOP
	MOVE TEMP 7 	BEGIN
		RETURN MINUS 1 TEMP 1	END
	MOVE TEMP 1 TEMP 7
	MOVE TEMP 9 PLUS 
	BEGIN
		HLOAD TEMP 10 TEMP 0 4
		RETURN TEMP 10
	END 1
	HSTORE TEMP 0 4 TEMP 9
	JUMP L0

L1
	NOOP
	MOVE TEMP 11 
	BEGIN
		HLOAD TEMP 12 TEMP 0 4
		RETURN TEMP 12
	END
	RETURN TEMP 11
END

