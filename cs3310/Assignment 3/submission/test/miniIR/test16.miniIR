MAIN
	MOVE TEMP 0 
	BEGIN
		MOVE TEMP 1 
		BEGIN
			RETURN CALL Create_Test()
		END
		HLOAD TEMP 2 TEMP 1 0
		HLOAD TEMP 3 TEMP 2 4

		RETURN CALL TEMP 3( TEMP 1 )
	END
	PRINT TEMP 0
END

Create_Test [0]
BEGIN
	MOVE TEMP 0 HALLOCATE 12
	HSTORE TEMP 0 0 Test_second
	HSTORE TEMP 0 4 Test_start
	HSTORE TEMP 0 8 Test_first

	MOVE TEMP 1 HALLOCATE 8
	HSTORE TEMP 1 0 TEMP 0

	MOVE TEMP 2 HALLOCATE 4
	MOVE TEMP 2 0
	HSTORE TEMP 1 4 TEMP 2

	RETURN TEMP 1
END

Test_start [1]
BEGIN
	MOVE TEMP 2 
	BEGIN
		RETURN CALL Create_Test()
	END
	MOVE TEMP 1 TEMP 2
	MOVE TEMP 3 10
	HSTORE TEMP 0 4 TEMP 3
	MOVE TEMP 4 PLUS 
	BEGIN
		HLOAD TEMP 5 TEMP 0 4
		RETURN TEMP 5
	END 
	BEGIN
		MOVE TEMP 6 
		BEGIN
			MOVE TEMP 7 
			BEGIN
				MOVE TEMP 8 
				BEGIN
					MOVE TEMP 9 TEMP 1
					HLOAD TEMP 10 TEMP 9 0
					HLOAD TEMP 11 TEMP 10 8

					RETURN CALL TEMP 11( TEMP 9 )
				END
				RETURN TEMP 8
			END
			HLOAD TEMP 12 TEMP 7 0
			HLOAD TEMP 13 TEMP 12 0

			RETURN CALL TEMP 13( TEMP 7 )
		END
		RETURN TEMP 6
	END
	HSTORE TEMP 0 4 TEMP 4
	MOVE TEMP 14 
	BEGIN
		HLOAD TEMP 15 TEMP 0 4
		RETURN TEMP 15
	END
	RETURN TEMP 14
END

Test_first [1]
BEGIN
	MOVE TEMP 1 
	BEGIN
		RETURN CALL Create_Test()
	END
	RETURN TEMP 1
END

Test_second [1]
BEGIN
	MOVE TEMP 1 PLUS 
	BEGIN
		HLOAD TEMP 2 TEMP 0 4
		RETURN TEMP 2
	END 10
	HSTORE TEMP 0 4 TEMP 1
	MOVE TEMP 3 
	BEGIN
		HLOAD TEMP 4 TEMP 0 4
		RETURN TEMP 4
	END
	RETURN TEMP 3
END

