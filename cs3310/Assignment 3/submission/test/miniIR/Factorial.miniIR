MAIN
	MOVE TEMP 0 
	BEGIN
		MOVE TEMP 1 
		BEGIN
			RETURN CALL Create_Fac()
		END
		HLOAD TEMP 2 TEMP 1 0
		HLOAD TEMP 3 TEMP 2 0
		MOVE TEMP 4 10
		RETURN CALL TEMP 3( TEMP 1 TEMP 4 )
	END
	PRINT TEMP 0
END

Create_Fac [0]
BEGIN
	MOVE TEMP 0 HALLOCATE 4
	HSTORE TEMP 0 0 Fac_ComputeFac

	MOVE TEMP 1 HALLOCATE 4
	HSTORE TEMP 1 0 TEMP 0

	RETURN TEMP 1
END

Fac_ComputeFac [2]
BEGIN
	CJUMP LT TEMP 1 1 L0
	MOVE TEMP 3 1
	MOVE TEMP 2 TEMP 3
	JUMP L1
L0
	MOVE TEMP 4 TIMES TEMP 1 
	BEGIN
		MOVE TEMP 5 
		BEGIN
			MOVE TEMP 6 TEMP 0
			HLOAD TEMP 7 TEMP 6 0
			HLOAD TEMP 8 TEMP 7 0
			MOVE TEMP 9 MINUS TEMP 1 1

			RETURN CALL TEMP 8( TEMP 6 TEMP 9 )
		END
		RETURN TEMP 5
	END
	MOVE TEMP 2 TEMP 4
L1
	NOOP
	MOVE TEMP 10 TEMP 2
	RETURN TEMP 10
END

