MAIN
	MOVE TEMP 0 
	BEGIN
		MOVE TEMP 1 
		BEGIN
			RETURN CALL Create_Test()
		END
		HLOAD TEMP 2 TEMP 1 0
		HLOAD TEMP 3 TEMP 2 0

		RETURN CALL TEMP 3( TEMP 1 )
	END
	PRINT TEMP 0
END

Create_Test [0]
BEGIN
	MOVE TEMP 0 HALLOCATE 8
	HSTORE TEMP 0 0 Test_start
	HSTORE TEMP 0 4 Test_next

	MOVE TEMP 1 HALLOCATE 8
	HSTORE TEMP 1 0 TEMP 0

	MOVE TEMP 2 HALLOCATE 4
	MOVE TEMP 2 0
	HSTORE TEMP 1 4 TEMP 2

	RETURN TEMP 1
END

Test_start [1]
BEGIN
	MOVE TEMP 2 10
	HSTORE TEMP 0 4 TEMP 2
	MOVE TEMP 3 
	BEGIN
		RETURN CALL Create_Test()
	END
	MOVE TEMP 1 TEMP 3
	MOVE TEMP 4 
	BEGIN
		MOVE TEMP 5 TEMP 1
		HLOAD TEMP 6 TEMP 5 0
		HLOAD TEMP 7 TEMP 6 4

		RETURN CALL TEMP 7( TEMP 5 )
	END
	HSTORE TEMP 0 4 TEMP 4
	MOVE TEMP 8 
	BEGIN
		HLOAD TEMP 9 TEMP 0 4
		RETURN TEMP 9
	END
	RETURN TEMP 8
END

Test_next [1]
BEGIN
	MOVE TEMP 1 20
	RETURN TEMP 1
END

