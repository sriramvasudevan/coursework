MAIN
	MOVE TEMP 0 
	BEGIN
		MOVE TEMP 1 
		BEGIN
			RETURN CALL Create_Ss()
		END
		HLOAD TEMP 2 TEMP 1 0
		HLOAD TEMP 3 TEMP 2 0
		MOVE TEMP 4 10
		MOVE TEMP 5 11
		MOVE TEMP 6 1
		RETURN CALL TEMP 3( TEMP 1 TEMP 4 TEMP 5 TEMP 6 )
	END
	PRINT TEMP 0
END

Create_Ss [0]
BEGIN
	MOVE TEMP 0 HALLOCATE 4
	HSTORE TEMP 0 0 Ss_Compute

	MOVE TEMP 1 HALLOCATE 8
	HSTORE TEMP 1 0 TEMP 0

	MOVE TEMP 2 HALLOCATE 4
	MOVE TEMP 2 0
	HSTORE TEMP 1 4 TEMP 2

	RETURN TEMP 1
END

Ss_Compute [4]
BEGIN
	MOVE TEMP 5 
	BEGIN
		MOVE TEMP 6 PLUS TEMP 1 TEMP 2
		MOVE TEMP 7 HALLOCATE TIMES 4 PLUS 1 TEMP 6
		HSTORE TEMP 7 0 TEMP 6
		MOVE TEMP 8 0
L0
		CJUMP LT TEMP 8 TEMP 6 L1
		MOVE TEMP 9 PLUS 1 TEMP 8
		MOVE TEMP 10 TIMES 4 TEMP 9
		MOVE TEMP 11 PLUS TEMP 7 TEMP 10
		HSTORE TEMP 11 0 0
		MOVE TEMP 8 PLUS TEMP 8 1
		JUMP L0
L1
		NOOP
		RETURN TEMP 7
	END
	HSTORE TEMP 0 4 TEMP 5
	MOVE TEMP 12 
	BEGIN
		MOVE TEMP 13 
		BEGIN
			HLOAD TEMP 14 TEMP 0 4
			RETURN TEMP 14
		END
		HLOAD TEMP 15 TEMP 13 0
		RETURN TEMP 15
	END

	MOVE TEMP 4 TEMP 12
	MOVE TEMP 16 TEMP 4
	PRINT TEMP 16
	MOVE TEMP 17 TEMP 4
	RETURN TEMP 17
END

