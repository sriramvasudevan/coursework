MAIN
	MOVE TEMP 0 1
	PRINT TEMP 0
END

Create_A [0]
BEGIN
	MOVE TEMP 0 HALLOCATE 4
	HSTORE TEMP 0 0 A_foo

	MOVE TEMP 1 HALLOCATE 12
	HSTORE TEMP 1 0 TEMP 0

	MOVE TEMP 2 HALLOCATE 4
	MOVE TEMP 2 0
	HSTORE TEMP 1 4 TEMP 2
	MOVE TEMP 3 HALLOCATE 4
	MOVE TEMP 3 0
	HSTORE TEMP 1 8 TEMP 3

	RETURN TEMP 1
END

A_foo [1]
BEGIN
	MOVE TEMP 2 0
	RETURN TEMP 2
END

Create_B [0]
BEGIN
	MOVE TEMP 0 HALLOCATE 4
	HSTORE TEMP 0 0 B_foo

	MOVE TEMP 1 HALLOCATE 8
	HSTORE TEMP 1 0 TEMP 0

	MOVE TEMP 2 HALLOCATE 4
	MOVE TEMP 2 0
	HSTORE TEMP 1 4 TEMP 2

	RETURN TEMP 1
END

B_foo [1]
BEGIN
	MOVE TEMP 2 0
	RETURN TEMP 2
END

