MAIN
	MOVE TEMP 0 
	BEGIN
		MOVE TEMP 1 
		BEGIN
			RETURN CALL Create_somename()
		END
		HLOAD TEMP 2 TEMP 1 0
		HLOAD TEMP 3 TEMP 2 0
		MOVE TEMP 4 6
		RETURN CALL TEMP 3( TEMP 1 TEMP 4 )
	END
	PRINT TEMP 0
END

Create_somename [0]
BEGIN
	MOVE TEMP 0 HALLOCATE 4
	HSTORE TEMP 0 0 somename_init

	MOVE TEMP 1 HALLOCATE 4
	HSTORE TEMP 1 0 TEMP 0

	RETURN TEMP 1
END

somename_init [2]
BEGIN
	MOVE TEMP 3 1
	MOVE TEMP 2 TEMP 3
	MOVE TEMP 4 	BEGIN
		RETURN MINUS 1 
	BEGIN
		MOVE TEMP 6 TEMP 2
		RETURN TEMP 6
	END	END
	MOVE TEMP 2 TEMP 4
	MOVE TEMP 7 0
	RETURN TEMP 7
END

