MAIN
	MOVE TEMP 0 
	BEGIN
		MOVE TEMP 1 
		BEGIN
			RETURN CALL Create_Ss()
		END
		HLOAD TEMP 2 TEMP 1 0
		HLOAD TEMP 3 TEMP 2 0
		MOVE TEMP 4 10
		MOVE TEMP 5 11
		RETURN CALL TEMP 3( TEMP 1 TEMP 4 TEMP 5 )
	END
	PRINT TEMP 0
END

Create_Ss [0]
BEGIN
	MOVE TEMP 0 HALLOCATE 4
	HSTORE TEMP 0 0 Ss_Compute

	MOVE TEMP 1 HALLOCATE 4
	HSTORE TEMP 1 0 TEMP 0

	RETURN TEMP 1
END

Ss_Compute [3]
BEGIN
	MOVE TEMP 5 10
	MOVE TEMP 4 TEMP 5
	MOVE TEMP 6 3
	MOVE TEMP 3 TEMP 6
	MOVE TEMP 7 
	BEGIN
		MOVE TEMP 8 PLUS TEMP 4 
		BEGIN
			MOVE TEMP 9 TEMP 3
			RETURN TEMP 9
		END
		RETURN TEMP 8
	END
	MOVE TEMP 1 TEMP 7
	MOVE TEMP 10 TEMP 1
	RETURN TEMP 10
END

